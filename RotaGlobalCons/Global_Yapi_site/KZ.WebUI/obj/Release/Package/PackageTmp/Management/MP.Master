<body>
    <form id="form1" runat="server">
        <asp:ScriptManager runat="server" ID="sm1"></asp:ScriptManager>
        <uc1:ucManagementPanelMenus runat="server" ID="ucManagementPanelMenus" />
        <div class="header">
            <div class="pull-left">
                <div class="logo"><span><%= ConfigurationManager.AppSettings["DomainName"] %></span></div>
                <div class="hamburger sidebar-toggle">
                    <span class="line"></span>
                    <span class="line"></span>
                    <span class="line"></span>
                </div>
            </div>
            <div class="pull-right p-r-15">
                <ul>
                    <li class="header-icon dib">
                        <label class="label label-info btn-lg">
                            <asp:Literal runat="server" ID="ltlLanguageName"></asp:Literal></label>
                    </li>
                    <li class="header-icon dib">
                        <img class="avatar-img" src="assets/images/avatar/1.jpg" alt="" />
                        <span class="user-avatar">
                            <asp:Literal ID="ltlNameSurname" runat="server"></asp:Literal>
                            <asp:HiddenField runat="server" ID="hfUserId" />
                            <i class="ti-angle-down f-s-10"></i></span>
                        <div class="drop-down dropdown-profile">
                            <div class="dropdown-content-body">
                                <ul>
                                    <li><a href="Profile"><i class="ti-user"></i><span>Profil</span></a></li>
                                    <li>
                                        <asp:LinkButton runat="server" ID="lnkLogOut" OnClick="lnkLogOut_OnClick" CausesValidation="False"><i class="ti-power-off"></i><span>Güvenli Çıkış</span></asp:LinkButton></li>
