        <script src="assets/js/lib/data-table/datatables-init.js"></script>
        <script src="assets/js/tagsinput.js"></script>

       

        <!-- scripit init-->
        <script type="text/javascript">
            function errorMessage(link) {
                sweetAlert("İşlem Başarısız", "Lütfen daha sonra tekrar deneyiniz veya sistem yöneticinize başvurunuz.", "error");
                $("#closeButton").click(function () {
                    window.location.href = link;
                });
            }

            function successMessage(link) {
                sweetAlert("İşlem Başarılı", "İşleminiz başarı ile gerçekleştirilmiştir.", "success");
                $("#closeButton").click(function () {
                    window.location.href = link;
                });
            }

            function deleteInfoMessage(link) {
                sweetAlert("Uyarı", "Silmek istediğiniz hizmete ait daha önceden verilen siparişler olduğu için bu hizmet silememektedir.", "error");
                $("#closeButton").click(function () {
                    window.location.href = link;
                });
            }

            function infoMessage(message) {
                sweetAlert("Uyarı", message, "error");
            }

            //Sadece rakam girişi sağlar.
            function isNumber(evt) {
                evt = (evt) ? evt : window.event;
                var charCode = (evt.which) ? evt.which : evt.keyCode;
                if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                    return false;
                }
                return true;
            }

            //Sadece rakam ve metin girişi sağlar.
            function isTextNumber(evt) {
                evt = (evt) ? evt : window.event;
                var charCode = (evt.which) ? evt.which : evt.keyCode;

                if (charCode === 46 || charCode === 32 || charCode === 33 || charCode === 58 || charCode === 33 || charCode === 40 || charCode === 41 || charCode === 47 || charCode === 38 || charCode === 63 || charCode === 44)
                    return true;

                if (charCode >= 64 && charcode <= 90 || charCode >= 48 && charCode <= 57) {
                    return true;
                }
                return false;
            }

            //Fiyat girişi sağlamaktadır.
            function isPrice(evt) {
                evt = (evt) ? evt : window.event;
                var charCode = (evt.which) ? evt.which : evt.keyCode;

                if (charCode === 44)
                    return true;

                if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                    return false;
                }
                return true;
            }
        </script>
        <asp:ContentPlaceHolder runat="server" ID="CustomCode" />
    </form>
</body>
</html>
